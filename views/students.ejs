<!--Student table-->
<style>
    table,th,td{
        border:1px solid black;/*Adds black border data*/
    }
</style>

<table>
    <tr>
    <th>Student ID</th>
    <th>Name</th>
    <th>Age</th>
    </tr>
    <% studentsList.forEach((student) =>{ %> 
        <tr>
            <td><%= student.sid %></td>
            <td><%= student.name %></td>
            <td><%= student.age %></td>
        </tr>
    <% }) %>
    </table>
    